},\n    data() {\n        return {\n            blog: content,\n            tag: null,\n        };\n    },\n    created() {},\n    computed: {\n        getLinks() {\n            var res = {};\n            this.blog.forEach(function(v) {\n                res[v.tag] = (res[v.tag] || 0) + 1;\n            });\n            // create object that shows how many of the same tags\n            return res;\n        },\n        createBlogData() {\n            // filter blog content based on tag\n\n            if (this.tag !== null) {\n                const results = [];\n                const toSearch = this.tag;\n                for (let i = 0; i < content.length; i++) {\n                    for (let key in content[i]) {\n                        if (content[i][key].indexOf(toSearch) != -1) {\n                            results.push(content[i]);\n                        }\n                    }\n                }\n                return results;\n            } else {\n                return content;\n            }\n        },\n    },\n    methods: {\n        getTagChild(value) {\n            this.tag = value;\n        },\n    },\n};\n</script>\n\n<style scoped lang=\"scss\">\n.blog {\n    display: flex;\n    justify-content: space-between;\n}\n</style>\n"]}]}